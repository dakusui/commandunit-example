---
"$extends":
  - base/normal.json
when:
  source:
    - ${COMMANDUNIT_HOSTFSROOT_MOUNTPOINT}/${COMMANDUNIT_PWD}/src/main/scripts/lib/core.sh
  cmd: assert_that
  args:
    - HELLO
    - "[["
    - hello
    - ==
    - Hello
    - "]]"
then:
  exitCode:
    - NOT_EQUAL
    - 0
  stdout:
    absent:
      - REGEX:.+
  stderr:
    present:
      - REGEX:Condition.*HELLO
      - REGEX:Detail.*hello.*Hello